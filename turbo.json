{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".svelte-kit/**", ".vercel/**"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true,
      "env": ["OPENAI_API_KEY", "YELP_API", "GOOGLE_PLACES_API_KEY"]
    }
  },
  "globalEnv": ["OPENAI_API_KEY", "YELP_API", "GOOGLE_PLACES_API_KEY"],
  "globalDependencies": ["OPENAI_API_KEY", "YELP_API", "GOOGLE_PLACES_API_KEY"]
}
